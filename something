--FUNCTION
function chopcombo()
	if activu == true then
		return;
	end;
	if chopcd == true then
		return;
	end;
	activu = true;
	chopcd = true;
	local player = game:GetService("Players").LocalPlayer
	local A_1 = game.Players.LocalPlayer.Character.Humanoid
	local A_2 = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	local A_3 = 0
	local A_4 = 0
	local A_5 = Vector3.new(0, 0, 0)
	local A_6 = "rbxassetid://241837157"
	local A_7 = 0
	local A_8 = Color3.new(255, 255, 255)
	local A_9 = "rbxassetid://3077287610"
	local A_10 = 1
	local A_11 = 999
	local Event = game:GetService("ReplicatedStorage").Damage
	Event:FireServer(A_1, A_2, A_3, A_4, A_5, A_6, A_7, A_8, A_9, A_10, A_11)
	if game.Lighting.TS.Value == false then
		game.Lighting.STWEffect.Enabled = true
		spawn(function()
			wait(.4)
			game.Lighting.STWEffect.Enabled = false
		end)
	end
	local player = game:GetService("Players").LocalPlayer
	local A_1 = game.Players.LocalPlayer.Character.Humanoid
	local A_2 = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	local A_3 = 0
	local A_4 = 0
	local A_5 = Vector3.new(0, 0, 0)
	local A_6 = "rbxassetid://241837157"
	local A_7 = 0
	local A_8 = Color3.new(255, 255, 255)
	local A_9 = "rbxassetid://3552355246"
	local A_10 = 1
	local A_11 = 10
	local Event = game:GetService("ReplicatedStorage").Damage
	Event:FireServer(A_1, A_2, A_3, A_4, A_5, A_6, A_7, A_8, A_9, A_10, A_11)
	for _, n in pairs(stand:GetChildren()) do
	      coroutine.resume(coroutine.create(function()
	        for _ = 1, 3 do
	          if n ~= rot and n.ClassName ~= "Sound" then
	            wait()
	            transparencyevent:FireServer(n, n.Transparency - 0.1)
	          end
	        end
	        if n ~= rot then
	          transparencyevent:FireServer(n, 0.6)
	        end
	      end))
		end
	attackpose:Play()
	activu = true
	human.JumpPower = 0
	chopanim:Play(0.1, 1, 1)
	trailevent:FireServer(SRA.Trail, true)
	trailevent:FireServer(SRA.Trail, true);
	wait(0.13);
	coroutine.resume(coroutine.create(function()
		for _ = 1,20 do
			wait()
			hito2STW(SRA, SRA.CFrame * CFrame.new(0, -1, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)), 3, 23, 0.35, 0.25, rot.CFrame.lookVector * 1, "rbxassetid://241837157", 0.075, Color3.new(255, 255, 255), "rbxassetid://260430079", math.random(9, 11) / 10, math.random(9, 11) / 10);	
		end;
	end));
	wait(0.13);
	coroutine.resume(coroutine.create(function()
		for _ = 1,15 do
			wait()
			hito2STW(SLA, SLA.CFrame * CFrame.new(0, -1, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)), 3, 23, 0.35, 0.25, rot.CFrame.lookVector * 1, "rbxassetid://241837157", 0.075, Color3.new(255, 255, 255), "rbxassetid://260430079", math.random(9, 11) / 10, math.random(9, 11) / 10);	
		end;
	end));
	wait(0.13);
	strongpunchevent:FireServer(Vector3.new(1, 0.5, 1), Vector3.new(0.3, 0.15, 0.3), BrickColor.new("Pastel Blue"));
	coroutine.resume(coroutine.create(function()
		for _ = 1,15 do
			wait()
			hito2STW(SRA, SRA.CFrame * CFrame.new(0, -1, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)), 3, 38, 0.75, 0.25, rot.CFrame.lookVector * 10, "rbxassetid://241837157", 0.075, Color3.new(255, 255, 255), "rbxassetid://260430079", math.random(9, 11) / 10, math.random(9, 11) / 10);		
		end;
	end));
	wait(0.475);
	trailevent:FireServer(SRA.Trail, false);
	wait(0.45)
	activu = false
	attackpose:Stop()
	human.JumpPower = 50
	for _, n in pairs(stand:GetChildren()) do
	      coroutine.resume(coroutine.create(function()
	        for _ = 1, 6 do
	           if n ~= rot and n.ClassName ~= "Sound" then
	            wait()
	            transparencyevent:FireServer(n, n.Transparency + 0.9)
	          end
	        end
	        if n ~= rot then
	          transparencyevent:FireServer(n, 1)
	        end
	      end))
		end
		trailevent:FireServer(SRA.Trail, false)
chopanim:Stop()		
	wait(2.5)
	chopcd = false
end
