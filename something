local EHMMM = 0
local blockcooldown = false
--FUNCTION BLOCK
function block()
	if blockcooldown == true then return end
	if activu == true then return end
	activu = true
	local player = game:GetService("Players").LocalPlayer
	local A_1 = game.Players.LocalPlayer.Character.Humanoid
	local A_2 = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	local A_3 = 0
	local A_4 = 0
	local A_5 = Vector3.new(0, 0, 0)
	local A_6 = "rbxassetid://241837157"
	local A_7 = 0
	local A_8 = Color3.new(255, 255, 255)
	local A_9 = "rbxassetid://3552355246"
	local A_10 = 1
	local A_11 = 10
	local Event = game:GetService("ReplicatedStorage").Damage
	Event:FireServer(A_1, A_2, A_3, A_4, A_5, A_6, A_7, A_8, A_9, A_10, A_11)
		for _, n in pairs(stand:GetChildren()) do
	      coroutine.resume(coroutine.create(function()
	        for _ = 1, 5 do
	          if n ~= rot and n.ClassName ~= "Sound" then
	            wait()
	            transparencyevent:FireServer(n, n.Transparency - 0.2)
	          end
	        end
	        if n ~= rot then
	          transparencyevent:FireServer(n, 0.6)
	        end
	      end))
		end
	local bep = true
	local de = mouse.KeyUp:connect(function(key)
	    if key == "x" then
	      bep = false
	    end
	end)
	blockanim:Play(0.1, 1,1)
	blockevent:FireServer(true)
	blockcooldown = true
	human.JumpPower = 0
	repeat
		wait()
	until bep == false
		for _, n in pairs(stand:GetChildren()) do
	      coroutine.resume(coroutine.create(function()
	        for _ = 1, 8 do
	          if n ~= rot and n.ClassName ~= "Sound" then
	            wait()
	            transparencyevent:FireServer(n, n.Transparency + 0.2)
	          end
	        end
	        if n ~= rot then
	          transparencyevent:FireServer(n, 1)
	        end
	      end))
		end
	blockevent:FireServer(false)
	blockanim:Stop(0.1)
	human.JumpPower = 50
	activu = false
	wait(3)
	blockcooldown = false	
end
