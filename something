					if v20 ~= rot then
						if v20.ClassName ~= "Sound" then
							if v20 ~= p1.StandSRA then
								if v20 ~= p1.StandLarm then
									if v20.ClassName ~= "IntValue" then
										wait();
										l__Transparency__7:FireServer(v20, v20.Transparency - 0.2);
										l__Transparency__7:FireServer(l__face__8, v20.Transparency - 0.2);
									end;
								end;
							end;
						end;
					end;
					if 0 <= 1 then
						if v21 < 5 then

						else
							break;
						end;
					elseif 5 < v21 then

					else
						break;
					end;
					v21 = v21 + 1;				
				end;
				if v20 ~= rot then
					if v20 ~= p1.StandSRA then
						if v20 ~= p1.StandLarm then
							l__Transparency__7:FireServer(v20, 0);
							l__Transparency__7:FireServer(l__face__8, 0);
						end;
					end;
				end;
			end));		
		end;
		wait(1)
		local trsevent = game.ReplicatedStorage.Transparency
