local leftorright = false
--FUNCTION Punch(Here some fixes
function punch()
if activu == true then return end
	activu = true
	punchsfx:Play()
	attackpose:Play()
	local player = game:GetService("Players").LocalPlayer
	local A_1 = game.Players.LocalPlayer.Character.Humanoid
	local A_2 = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	local A_3 = 0
	local A_4 = 0
	local A_5 = Vector3.new(0, 0, 0)
	local A_6 = "rbxassetid://241837157"
	local A_7 = 0
	local A_8 = Color3.new(255, 255, 255)
	local A_9 = "rbxassetid://3552355246"
	local A_10 = 1
	local A_11 = 6
	local Event = game:GetService("ReplicatedStorage").Damage
	Event:FireServer(A_1, A_2, A_3, A_4, A_5, A_6, A_7, A_8, A_9, A_10, A_11)
		for _, n in pairs(stand:GetChildren()) do
	      coroutine.resume(coroutine.create(function()
	        for _ = 1, 5 do
	          if n ~= rot and n.ClassName ~= "Sound" then
	            wait()
	            transparencyevent:FireServer(n, n.Transparency - 0.2)
	          end
	        end
	        if n ~= rot then
	          transparencyevent:FireServer(n, 0.6)
	        end
	      end))
		end
	human.JumpPower = 0
	if leftorright == false then
		leftorright = true
		rightpunchanim:Play(0.1, 1, 1)
		trailevent:FireServer(SRA.Trail, true)
		wait(0.2)
		for _, n in pairs(stand:GetChildren()) do
	      coroutine.resume(coroutine.create(function()
	        for _ = 1, 3 do
	           if n ~= rot and n.ClassName ~= "Sound" and n ~= stand["Stand Head"] then
	            wait()
	            transparencyevent:FireServer(n, n.Transparency + 0.2)
	          end
	        end
	        if n ~= rot and n ~= stand["Stand Head"] then
	          transparencyevent:FireServer(n, 1)
	        end
	      end))
		end			
		coroutine.resume(coroutine.create(function()
		for _ = 1, 5 do
		wait()
		hitoSTW(SRA, SRA.CFrame * CFrame.new(0, -1, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2.5, 11, 0.5, 0.25, rot.CFrame.lookVector * 10, "rbxassetid://241837157", 0.075, Color3.new(255, 255, 255),"rbxassetid://260430079", math.random(9, 11) / 10, math.random(9, 11) / 25)
		end
		end))
	elseif leftorright == true then
		leftorright = false
		leftpunchanim:Play(0.1, 1, 1)
		trailevent:FireServer(SLA.Trail, true)
		wait(0.2)
		for _, n in pairs(stand:GetChildren()) do
	      coroutine.resume(coroutine.create(function()
	        for _ = 1, 3 do
	           if n ~= rot and n.ClassName ~= "Sound" then
	            wait()
	            transparencyevent:FireServer(n, n.Transparency + 0.2)
	          end
	        end
	        if n ~= rot then
	          transparencyevent:FireServer(n, 1)
	        end
	      end))
		end			
		coroutine.resume(coroutine.create(function()
		for _ = 1, 5 do
		wait()
		hito(SLA, SLA.CFrame * CFrame.new(0, -1, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2.5, 11, 0.5, 0.25, rot.CFrame.lookVector * 10, "rbxassetid://241837157", 0.075, Color3.new(255, 255, 255),"rbxassetid://260430079", math.random(9, 11) / 10, math.random(9, 11) / 25)
		end
		end))
	end
	wait(0.35)
	attackpose:Stop()
	human.JumpPower = 50
	activu = false
	trailevent:FireServer(SRA.Trail, false)
	trailevent:FireServer(SLA.Trail, false)
end
