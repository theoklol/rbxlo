--SETS HITS

function hitoflesh()
local chr = game.Players.LocalPlayer.Character
for _, guy in pairs(workspace.Entities:GetChildren()) do
	if guy:FindFirstChild("Humanoid") then
			if guy:FindFirstChild("HumanoidRootPart") then
				if guy ~= chr then
					if (guy:FindFirstChild("HumanoidRootPart").Position - game:GetService("Players").LocalPlayer.character.Head.Position).magnitude < 4.4 then
						if guy:FindFirstChild("HumanoidRootPart"):FindFirstChild("alabo") == nil then
							local human0 = guy:FindFirstChild("Humanoid");
							print(guy)
							local A_1 = human0
							local A_2 = guy:FindFirstChild("HumanoidRootPart").CFrame
							local A_3 = 25
							local A_4 = 0.2
							local A_5 = Vector3.new(0, 0, 0)
							local A_6 = "rbxassetid://241837157"
							local A_7 = 0
							local A_8 = Color3.new(255, 255, 255)
							local A_9 = "rbxassetid://566593606"
							local A_10 = 1
							local A_11 = 999
							local Event = game:GetService("ReplicatedStorage").Damage
							Event:FireServer(A_1, A_2, A_3, A_4, A_5, A_6, A_7, A_8, A_9, A_10, A_11)
						end
					end
				end
			end
		end
	end
end
