local bloodsuckcd = false
--FUNCTION BLOODSUCK
function bloodsuck()
	if activu == true then return end
	if bloodsuckcd == true then return end
	bloodsuckcd = true
	activu = true
	human.JumpPower = 0
	bloodsuckanim:Play(0.1, 1, 1)
	wait(0.5)
	punchsfx:Play()
	coroutine.resume(coroutine.create(function()
		for _ = 1, 10 do
			wait()
			hito3STW(SRA, 6.5, 30, 0.5, 0.25, rot.CFrame.lookVector * 0)
		end
	end))
	wait(0.65)
		activu = false
	wait(3)
	bloodsuckcd = false
end
