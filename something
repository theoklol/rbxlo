timestopcooldown = false
--FUNCTION timestop
function timestop()
lol = false
if activu == true then return end
if game.Lighting.TS.Value == true then return end
if game.Lighting.TSing.Value == true then return end
if game.Lighting.TS.Value == true then return end
	activu = true
	timestopcooldown = true
	activu = false
	timestopevent:FireServer(5, "ShadowDio")
	spawn(function()
	repeat
	wait()
	l__Anchor__85:FireServer(ST, false);
		l__Anchor__85:FireServer(SRA, false);
		l__Anchor__85:FireServer(SLA, false);
		l__Anchor__85:FireServer(SH, false);
		l__Anchor__85:FireServer(l__Torso__79, false);
		l__Anchor__85:FireServer(u86, false);
		l__Anchor__85:FireServer(u87, false);
		l__Anchor__85:FireServer(u88, false);
		l__Anchor__85:FireServer(u89, false);
		l__Anchor__85:FireServer(l__Head__69, false);
		until lol == true
		end)
	human.WalkSpeed = 18
	human.JumpPower = 50
		wait(5)
		lol = true
		untimestopevent:FireServer()
	wait(25)
	timestopcooldown = false
end
