--FUNCTION STWRTZ
function STWRTZ()
	if activu == true then return end
	if RTZSTW == false then 
	RTZAnim:Play(0.1,1,1)
	human.JumpPower = 0
	human.WalkSpeed = 0
	ShadowTheWorldRTZ:FireServer(true)
	print("stwrtz has been made true.")
	wait(1.5)
	RTZSTW = true
	human.HealthChanged:connect(function()
	if RTZSTW == false then return end
		hitoSTWRTZ()
		RTZSTW = false
		human.JumpPower = 50
		human.WalkSpeed = 18
		RTZAnim:Stop(0.1)
	end)
	elseif RTZSTW == true then
		RTZAnim:Stop(0.1)
		ShadowTheWorldRTZ:FireServer(false)
		print("stwrtz has been made false.")
		human.JumpPower = 50
		human.WalkSpeed = 18
		RTZCool = 0
		activu = false
		wait(1.5)
		RTZSTW = false
	end
end
